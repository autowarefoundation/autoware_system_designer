<?xml version="1.0"?>
<launch>
  <!-- This launch file is auto-generated by the autoware_system_designer -->
  <!-- Compute Unit Launcher: {{ compute_unit }} -->
  <!-- This launcher starts all components assigned to compute unit: {{ compute_unit }} -->

{% for arg_name in forward_args %}
  <arg name="{{ arg_name }}"/>
{% endfor %}

  <let name="root_dir" value="$(dirname)"/>

{% for component_info in components %}
  <!-- Include component {{ component_info.component }} -->
  <include file="$(var root_dir)/{{ component_info.component }}/{{ component_info.component }}.launch.xml">
{% for arg_name in component_info.args %}
    <arg name="{{ arg_name }}" value="$(var {{ arg_name }})"/>
{% endfor %}
  </include>

{% endfor %}
</launch>
