(function() {
  // Initialize sequence diagram data namespace
  if (!window.sequenceDiagramData) window.sequenceDiagramData = {};
  
  // Store sequence diagram Mermaid syntax for the specific mode
  {% set mermaid_syntax %}
  {% include "visualization/diagram/sequence_diagram_mermaid.jinja2" %}
  {% endset %}
  window.sequenceDiagramData["{{ mode if mode else 'default' }}"] = {{ mermaid_syntax | tojson }};
})();
